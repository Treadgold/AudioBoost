{
  "description": "Increase the volume of audio on a tab, making quiet sounds louder and limiting loud sounds.",
  "manifest_version": 2,
  "name": "up-to-eleven",
  "version": "0.2",
  "icons": {
    "48": "icons/uptoeleven-disabled.png",
    "96": "icons/uptoeleven-disabled.png"
  },
  "background": {
    "scripts": ["up-to-eleven.js"]
  },
  "browser_action": {
    "default_icon": {
      "48": "icons/uptoeleven-disabled.png",
      "96": "icons/uptoeleven@2x-disabled.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "options_ui": {
    "page": "options.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["up-to-eleven.js"]
    }
  ]
}
